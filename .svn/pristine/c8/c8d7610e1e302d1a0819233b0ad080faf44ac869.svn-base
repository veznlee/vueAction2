import request from '../utils/request'

export function fetchList(query) {
    return request({
        url: '/article/list',
        method: 'get',
        params: query
    })
}

//menu sideBar
export function menuList(param) {
    return request({
        url: '/user/userPermission',
        method: 'get',
        params: param
    })
}

