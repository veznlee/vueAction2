    white-space: nowrap;
    height:24px;
}
.p-direct-workflow .before-bg,
.p-direct-workflow:before{
    content: "";
    position: absolute;
    top:0;
    left:5px;
    right:5px;
    bottom:0;
    z-index:0;
    margin: auto;
    height: 6px;
    border: 1px solid #c0df7c;
    border-radius: 6px;
    background: #c0df7c;
}
.p-direct-workflow .before-bg{
    background-color: #c0df7c;
    right: auto;
}
.p-direct-workflow .list{
    display: inline-block;
    margin: 0 5px;
    border-radius: 15px;
    border: 1px solid #c0df7c;
    line-height: 0;
    color: #fff;
    z-index: 1;
    position: relative;
    width: 60px;
    font-size: 12px;
    height: 22px;
    line-height: 22px;
    background: #fff;
    color: #000;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.p-direct-workflow .checked{
    background-color: #c0df7c;
    color: #fff;
}
