.f-prz{position:relative;zoom:1;}
.f-pa{position:absolute;}
.f-pa-full{position:absolute;left:0;top:0;right:0;bottom:0;}
.f-pa-t{position:absolute;top:0;left:0;right:0;}
.f-pa-b{position:absolute;bottom:0;left:0;right:0;}
.f-pa-l{position:absolute;left:0;top:0;bottom:0;}
.f-pa-r{position:absolute;right:0;top:0;bottom:0;}
.f-full-body{width:100%;height:100%;}
.f-oh{overflow:hidden;}
.f-hidden,.f-dsn{display:none !important;}
.f-oya{overflow-y: auto;}
.f-oxa{overflow-x: auto;}
.f-oyh{overflow-y: hidden;}
.f-oxh{overflow-x: hidden;}
.f-ff0{font-family:arial,\5b8b\4f53;}
.f-ff1{font-family:"Microsoft YaHei",\5fae\8f6f\96c5\9ed1,arial,\5b8b\4f53;}
.f-fs0{font-size:0;}
.f-fs12{font-size:12px;}
.f-fs14{font-size:14px;}
.f-fs16{font-size:16px;}
.f-fs18{font-size:18px;}
.f-fwn{font-weight:normal;}
.f-fwb{font-weight:bold;}
.f-tal{text-align:left;}
.f-tac{text-align:center;}
.f-tar{text-align:right;}
.f-taj{text-align:justify;text-justify:inter-ideograph;}
.f-vam,.f-vama *{vertical-align:middle;}
.f-wsn{word-wrap:normal;white-space:nowrap;}
.f-pre{overflow:hidden;text-align:left;white-space:pre-wrap;word-wrap:break-word;word-break:break-all;}
.f-wwb{white-space:normal;word-wrap:break-word;word-break:break-all;}
.f-ti{overflow:hidden;text-indent:-30000px;}
.f-ti2{text-indent:2em;}
.f-lhn{line-height:normal;}
.f-tdu,.f-tdu:hover{text-decoration:underline;}
.f-tdn,.f-tdn:hover{text-decoration:none;}
.f-toe{overflow:hidden;word-wrap:normal;white-space:nowrap;text-overflow:ellipsis;}
.f-csp{cursor:pointer;}
.f-csd{cursor:default;}
.f-csh{cursor:help;}
