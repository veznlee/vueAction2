    background-color:#5d9fcd;
}
.btn-green{
    background-color:#90c31f;
}
.btn-yellow{
    background-color:#f4bc35;
}
.btn-orange{
    background-color:#fc9746;
}
.btn-gray,
.btn-blue.disabled,
.btn-green.disabled,
.btn-yellow.disabled,
.btn-orange.disabled{
    color:#fff;
    background-color:#a8a8a8;
}
.btn-blue:hover,.btn-blue:focus,.btn-blue:active,.btn-blue.active{
