.f-usn{-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;-o-user-select:none;user-select:none;}
/*公用类-常用间距控制*/
.f-mh-0{margin-top:0 !important;margin-bottom:0 !important;}
.f-mt-0{margin-top:0 !important;}
.f-mt-10{margin-top:10px !important;}
.f-mb-0{margin-bottom:0 !important;}
.f-mb-10{margin-bottom:10px !important;}
.f-mr-10{margin-right:10px !important;}
.f-ml-10{margin-left:10px !important;}
/*公用类-清除浮动*/
.clearfix:after {content: "."; display: block; height:0; clear:both; visibility: hidden;}
.clearfix { *zoom:1; }
.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th{
	padding:5px;
}
/*公用类-垂直居中*/
.vertical-center-unknown{vertical-align: middle;/*垂直居中*/white-space:normal;/*强制换行*/word-break:break-all;/*英文强制换行*/text-align: center;	}
.vertical-center-unknown > *{	display: inline-block;vertical-align: middle;margin: 0;padding: 0;}
.vertical-center-unknown:after{content: "";display: inline-block;height: 100%;	width: 0;vertical-align: middle;visibility: hidden;}
/*公用组件--未登录操作拦截*/
.shadow-absolute-layout{
	position:absolute;
	left:0;
	top:0;
	bottom:0;
	right:0;
	background:rgba(0, 0, 0, 0.4);
	z-index:999999;
}
.layout-to-login{
	position:absolute;
	left:50%;
	top:40%;
	width:300px;
	height:100px;
	margin-left:-150px;
	background:#fff;
	border-radius: 10px;
	box-shadow: 0 0 10px #00000099;
	color:#3e7d97;
	text-align: center;
}
.layout-to-login .tip{
	padding:15px 0 10px;
}
.layout-to-login .tip,.layout-to-login .tip i{
	font-size:30px;
}
.layout-to-login .tip i{
	margin-right:10px;
}
.layout-to-login .msg{
	font-size:18px;
}
.layout-to-login .msg a{
	color:#0d92d4;
	cursor: pointer;
}
.layout-to-login .msg a:hover{
