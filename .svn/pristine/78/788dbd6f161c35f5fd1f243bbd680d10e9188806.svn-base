/*index css*/
.layout-wrap{
    box-shadow: 1px 1px 3px #ddd;
    border-radius: 0 0 5px 5px;
    margin-bottom: 15px;
    background-color: #fff;
}
.layout-green .layout-title{
    border-left-color: #729b18;
}
.layout-green .layout-title h4{
    color: #729b18;
}
.layout-blue .layout-title{
    border-left-color: #3487c0;
}
.layout-blue .layout-title h4{
    color: #3487c0;
}
.layout-title{
    border-left: 6px solid #ddd;
    border-bottom: 1px solid #ddd;
    padding: 8px 0 8px 15px;
}
.layout-title h4{
    font-size: 16px;
    color: #333;
}
.layout-content{
    padding: 15px;
}
.content-green .box-title{
    background-color: #a6cf4c;
}
.content-green .box-title h4 .el-badge__content{
    background-color: #fcff00;
}
.content-green .box-title h4 .el-badge__content{
    background-color: #fcff00;
    color: #729b18;
}
.content-green .box-title .more{
    border-left-color: #c4e776;
    background-color: #8fbc2b;
}

.box-title{
    background-color: #f1f1f1;
    border-radius: 10px 10px 0 0;
    display: flex;
    align-items: center;
}
.box-title h4{
    font-size: 16px;
    padding: 8px 0 8px 15px;
    flex: 1;
    color: #fff;
}
.el-badge{
    font-size: 0;
}
.el-badge__content{
    margin-left: 8px;
    font-size: 14px;
    background-color: #f1f1f1;
    color: #333;
    border: none;
    font-weight: bold;
}
.box-title .more{
    text-align: right;
    color: #fff;
    border-left: 2px solid #ddd;
    display: block;
    padding: 10px 15px;
    border-radius: 0 10px 0 0;
    background-color: #f1f1f1;
}
.content-blue .box-title{
    background-color: #41a1e3;
}
.content-blue .el-badge__content{
    background-color: #fff;
    color: #ff7100;
}
.content-blue .box-content{
    border: 1px solid #99cef2;
    border-top: none;
}
.box-content .el-table__body{
    text-align: center;
    font-size: 14px;
}
.box-content .head{
    background-color: #bae3ff;
    display: flex;
}
.box-content .head .headTitle{
    flex: 1;
    text-align: center;
    padding: 10px 0;
    color: #196ca6;
}
.box-content .head .headRight{
}
.box-content .head .line{
    width: 1px;
    background-color: #68b7ee;
}

.iconShow{
    display: flex;
    align-items: center;
    padding: 20px 0;
}
.iconShow .iconItem{
    flex: 1;
    text-align: center;
}
.iconShow .iconItem p{
    margin-bottom: 10px;
}
.iconShow .iconItem p i{
    color: #54b6f9;
    font-size: 70px;
}
.iconShow .iconItem span{
    font-size: 16px;
}

.layout-content .box-title .goods-count{
    margin-left: 15px;
    font-size: 14px;
}
.layout-independent{
    padding: 0;
    box-shadow: 1px 1px 3px #ddd;
    border-radius: 10px;
    margin-bottom: 15px;
}
.content-orange .box-title{
    background-color: #e8a544;
}
.content-orange .box-title h4 .el-badge__content{
    background-color: #fcff00;
}
.content-orange .box-title h4 .el-badge__content{
    background-color: #fcff00;
    color: #729b18;
}
.content-orange .box-title .more{
    border-left-color: #ffd08c;
    background-color: #d8922d;
}
.goods-box{
    padding: 15px;
    padding-top: 0;
    border: 1px solid #eee;
    border-radius: 0 0 10px 10px;
}
.goods-box .good{
    display: flex;
    align-items: center;
    padding: 15px 0;
    margin-top: 15px;
    border: 1px solid #ddd;
}
.goods-box .good .goodImg{
    text-align: center;
    min-width: 120px;
}
.goods-box .good .goodImg img{
    height: 120px;
}
.goods-box .good .goodInfo{
    flex: 1;
}
.goods-box .good .goodInfo li{
    padding: 5px 0;
    color: #666;
}
.goods-box .good .goodInfo li h4{
    font-size: 16px;
    color: #333;
}
.goods-box .good .goodInfo li b{
    margin-left: 10px;
    color: #333;
}
.goods-box .good .goodInfo li .surplus{
    color: #e8a544;
}
.dialogTop{
    background-color: #e8f5cb;
    padding: 8px 0;
    margin: -10px -20px 20px !important;
}
.formInput input{
    text-align: center;
}
